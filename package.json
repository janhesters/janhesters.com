  "husky": {
    "hooks": {
      "pre-commit": "npm run -s validate"
    }
  },
  "name": "janhesters",
  "private": true,
  "resolutions": {
    "webpack": "^5.0.0-beta.22"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "echo 'Linting ...' && npm run -s lint --fix && echo 'Lint complete.'",
    "functional-tests": "testcafe chrome src/**/*functional.test.js --app 'yarn dev' --app-init-delay 2000",
    "lint": "eslint --ignore-path .gitignore .",
    "start": "next start",
    "unit-tests": "NODE_ENV=test babel-node ./src/tests/index.test.js",
    "validate": "npm run -s unit-tests && npm run -s lint && npm run -s functional-tests",
    "watch": "watch 'clear && npm run -s unit-tests | tap-nirvana && npm run -s format' src"
  },
  "version": "0.1.0"
}
